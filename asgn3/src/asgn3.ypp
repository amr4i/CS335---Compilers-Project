/*B.1.8 Literals*/ 
literal : boolean_literal 	{}
		| integer_literal 	{}
		| character_literal 	{}
		| string_literal 	{}
		| null_literal	{}
		;
boolean_literal : TRUE 	{}
		| FALSE	{}
		;
integer_literal : DINT_LITERAL 	{}
		| HDINT_LITERAL	{}
		;

character_literal : CHAR_LITERAL	{}
		;
		
/*
simple_escape_sequence : 	SIMPLE_ESC_SEQ 	{}
							;
*/

string_literal : REG_STR_LITERAL 	{}
		| VER_STR_LITERAL	{}
		;

null_literal :	NULL_LIT	{}
		;

